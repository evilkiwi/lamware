{
  "private": true,
  "name": "@lamware/example-closure",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf build",
    "compile": "NODE_ENV=production ts-node dev/build.ts",
    "dev": "nodemon dev/index.ts",
    "start-dev": "concurrently \"yarn watch\" \"yarn dev\"",
    "watch": "NODE_ENV=development ts-node dev/build.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.1277.0",
    "lambda-warmer": "^1.2.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/cookie": "^0.5.1",
    "@types/cors": "^2.8.13",
    "@types/vhost": "^3.0.4",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lambda-local": "^2.0.3",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "vhost": "^3.0.2"
  }
}
